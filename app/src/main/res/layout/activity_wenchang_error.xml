                    android:layout_marginLeft="10dp"
                    android:src="@mipmap/wenxue"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" />
                <TextView
                    android:id="@+id/wenxue"
                    android:layout_toRightOf="@+id/image"
                    android:gravity="center"
                    android:layout_marginLeft="10dp"
                    android:text="文学"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" />

                <TextView
                    android:text="10"
                    android:layout_toLeftOf="@+id/me_item_lottery_iv_arrow_right"
                    android:gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" />


                <ImageView
                    android:id="@+id/me_item_lottery_iv_arrow_right"
                    style="@style/me_common_item_arrow_right_imageView_style" />
            </RelativeLayout>
            <View
                android:id="@+id/view5"
                android:layout_below="@+id/wen_one"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/cp_shadow"/>
