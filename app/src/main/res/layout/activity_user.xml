            android:gravity="center"
            android:src="@mipmap/mine_art"
            android:layout_marginLeft="10dp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent" />
        <TextView
            android:textColor="@color/black"
            android:gravity="center"
            android:layout_marginLeft="21dp"
            android:text="艺考方向"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/name"
            android:layout_toEndOf="@+id/name"
            android:layout_marginStart="21dp" />

        <TextView
            android:id="@+id/boxin_value"
            android:layout_toLeftOf="@+id/me_item_lottery_iv_arrow_right"
            android:text="播音"
            android:textColor="@color/black"
            android:gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="match_parent" />
        <ImageView
            android:id="@+id/me_item_lottery_iv_arrow_right"
            style="@style/me_common_item_arrow_right_imageView_style" />
    </RelativeLayout>
    <View
        android:id="@+id/view5"
        android:layout_marginLeft="10dp"
        android:layout_below="@+id/done"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/cp_shadow"/>
    <RelativeLayout
        android:id="@+id/dfive"
        android:background="@color/white_text"
        android:layout_below="@+id/reboundScrollView1"
        android:layout_width="match_parent"
        android:layout_height="50dp">
        <ImageView
            android:id="@+id/name"
            android:gravity="center"
            android:src="@mipmap/mine_gradle"
            android:layout_marginLeft="10dp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent" />
        <TextView
